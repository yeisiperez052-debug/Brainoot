local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")

local PlaceId = game.PlaceId
local JobIdCache = {}

-- Funci√≥n para obtener servidores con 0 jugadores
function findEmptyServer()
    local url = "https://games.roblox.com/v1/games/"..PlaceId.."/servers/Public?sortOrder=1&limit=100"
    
    local success, response = pcall(function()
        return game:HttpGet(url)
    end)
    
    if success then
        local data = HttpService:JSONDecode(response)
        for _, server in ipairs(data.data) do
            if server.playing == 0 and server.id ~= game.JobId then
                print("‚úÖ Servidor vac√≠o encontrado: "..server.id)
                return server.id
            end
        end
    end
    return nil
end

-- Funci√≥n para unirse a servidor vac√≠o
function joinEmptyServer()
    print("üîç Buscando servidor vac√≠o...")
    local emptyJobId = findEmptyServer()
    
    if emptyJobId then
        print("üöÄ Uni√©ndose al servidor vac√≠o...")
        TeleportService:TeleportToPlaceInstance(PlaceId, emptyJobId, Players.LocalPlayer)
    else
        print("‚ùå No hay servidores vac√≠os, creando uno nuevo...")
        TeleportService:Teleport(PlaceId)
    end
end

