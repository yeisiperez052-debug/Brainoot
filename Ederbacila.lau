local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")

local function joinEmpty()
    local placeId = game.PlaceId
    
    while true do
        task.wait(5)
        
        local url = "https://games.roblox.com/v1/games/"..placeId.."/servers/Public?sortOrder=1&limit=100"
        
        local success, response = pcall(function()
            return game:HttpGet(url)
        end)
        
        if success then
            local data = HttpService:JSONDecode(response)
            
            for _, server in ipairs(data.data) do
                if server.playing <= 1 and server.id ~= game.JobId then
                    print("üéØ Servidor con "..server.playing.." jugadores encontrado")
                    print("üöÄ Teleportando...")
                    
                    TeleportService:TeleportToPlaceInstance(placeId, server.id)
                    task.wait(5)
                    return
                end
            end
        end
    end
end

-- Verificar si hay jugadores
if #game.Players:GetPlayers() > 1 then
    print("üë• Hay otros jugadores, buscando servidor vac√≠os...")
    joinEmpty()
